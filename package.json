{
	"name": "@proadject/proadject",
	"version": "0.0.1",
	"scripts": {
		"build:all": "npm run build:common && npm run build:demo && npm run build:lcu",
		"build:common": "ng build common",
		"build:demo": "ng build demo --prod",
		"build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
		"demo": "npm run build:common && npm run start:demo",
		"demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
		"deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
		"deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
		"deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
		"ng": "ng",
		"pack": "npm run pack:lcu",
		"pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/proadject.lcu.js",
		"pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
		"pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
		"pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
		"start": "ng serve demo --port=4293",
		"build": "npm run build:common && npm run build:lcu",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "^10.2.5",
		"@angular/cdk": "^12.1.3",
		"@angular/common": "^12.1.3",
		"@angular/compiler": "^12.1.3",
		"@angular/core": "^12.1.3",
		"@angular/elements": "^12.1.3",
		"@angular/fire": "^6.1.5",
		"@angular/flex-layout": "^11.0.0-beta.33",
		"@angular/forms": "^12.1.3",
		"@angular/material": "^12.1.3",
		"@angular/platform-browser": "^12.1.3",
		"@angular/platform-browser-dynamic": "^12.1.3",
		"@angular/router": "^12.1.3",
		"@aspnet/signalr": "^1.1.4",
		"@lcu/common": "^1.84.735",
		"@lowcodeunit-devkit/ng-lcu": "^1.28.161",
		"@lowcodeunit/lcu-icons-common": "^1.2.56",
		"@types/highlight.js": "^10.1.0",
		"@types/tinycolor2": "^1.4.3",
		"@webcomponents/custom-elements": "^1.4.3",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"copy": "^0.3.2",
		"hammerjs": "^2.0.8",
		"highlight.js": "^11.1.0",
		"material-design-icons": "^3.0.1",
		"ngx-build-plus": "^12.2.0",
		"rxjs": "^7.2.0",
		"sass.js": "^0.11.1",
		"terser": "^5.7.1",
		"tinycolor2": "^1.4.2",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^12.1.3",
		"@angular-devkit/build-ng-packagr": "~0.1002.0",
		"@angular/cli": "~12.1.3",
		"@angular/compiler-cli": "^12.1.3",
		"@angular/language-service": "^12.1.3",
		"@lcu/cli": "^1.7.97",
		"@lowcodeunit/lcu-documentation-common": "^1.6.38",
		"@types/jasmine": "~3.8.1",
		"@types/jasminewd2": "~2.0.10",
		"@types/node": "^16.4.1",
		"@types/tinycolor2": "^1.4.3",
		"codelyzer": "^6.0.2",
		"concat-glob-cli": "^0.1.0",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.8.0",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "~6.3.4",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.3",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.7.0",
		"mkdirp": "^1.0.4",
		"ng-momentum": "0.0.24",
		"ng-packagr": "^12.1.2",
		"protractor": "^7.0.0",
		"rimraf": "^3.0.2",
		"scss-to-json": "^2.0.0",
		"ts-node": "~10.1.0",
		"tsickle": "^0.43.0",
		"tslint": "~6.1.3",
		"typed-scss-modules": "^4.1.1",
		"typescript": "^4.3.5"
	}
}
